<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!--    saldo-->
    <RelativeLayout
        android:id="@+id/rlUp"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:background="@color/skyblue">

        <TextView
            android:id="@+id/tvFName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:fontFamily="@font/bold"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="30sp" />

        <ImageView
            android:id="@+id/iv1"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:layout_below="@+id/tvFName"
            android:layout_marginTop="10dp"
            android:src="@drawable/wallet"/>

        <TextView
            android:id="@+id/tv1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/iv1"
            android:text="BALANCE"
            android:textSize="40sp"
            android:gravity="center"
            android:textColor="@color/white"
            android:fontFamily="@font/bold"/>

        <TextView
            android:id="@+id/tvSaldo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv1"
            android:layout_marginTop="20dp"
            android:text="IDR "
            android:textSize="25sp"
            android:gravity="center"
            android:textColor="@color/white"
            android:fontFamily="@font/reguler"/>

    </RelativeLayout>
    <!--end of saldo code layout-->
    <!--    wash or salon menu-->
    <LinearLayout
        android:id="@+id/rlDown"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rlUp"
        android:layout_marginTop="-100dp"
        android:padding="30dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:background="@drawable/shape_main">

        <!--            car wash-->
        <RelativeLayout
            android:layout_width="150dp"
            android:layout_height="170dp"
            android:layout_margin="20sp">

            <ImageButton
                android:id="@+id/btn_carwash"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/button_menu_shape"
                android:paddingLeft="20dp"
                android:paddingRight="20dp"
                android:paddingTop="30dp"
                android:scaleType="centerInside"
                android:src="@drawable/carwash" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Car Wash"
                android:paddingTop="10dp"
                android:fontFamily="@font/bold"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:gravity="center"/>
        </RelativeLayout>
        <!--end of carwash-->

        <!--            carsalon-->
        <RelativeLayout
            android:layout_width="150dp"
            android:layout_height="170dp"
            android:layout_margin="20sp">
            <ImageButton
                android:id="@+id/btn_salon"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/button_menu_shape"
                android:paddingLeft="20dp"
                android:paddingRight="20dp"
                android:paddingTop="30dp"
                android:scaleType="centerInside"
                android:src="@drawable/carsalon" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Car Salon"
                android:paddingTop="10dp"
                android:fontFamily="@font/bold"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:gravity="center"/>
        </RelativeLayout>
    </LinearLayout>
    <!--    end of wash or salon menu-->
</RelativeLayout>

