<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".RegisterCarSalonActivity">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/black"
            android:text="Register Car Salon"
            android:textColor="@color/skyblue"
            android:textSize="35sp"
            app:layout_constraintBottom_toTopOf="@+id/tvNameCS"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.4" />

        <TextView
            android:id="@+id/tvNameCS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/reguler"
            android:text="Car Salon Name :"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/etnama"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.28"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/etnama"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/input_field"
            android:fontFamily="@font/reguler"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:hint="Car Salon Name "
            android:inputType="text"
            app:layout_constraintBottom_toTopOf="@+id/tvalamatCS"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/tvalamatCS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:fontFamily="@font/reguler"
            android:text="Car Salon Address :"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/llRegisCS"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.29"
            app:layout_constraintStart_toStartOf="parent" />
        <RelativeLayout
            android:id="@+id/llRegisCS"
            android:layout_width="250dp"
            android:layout_height="27dp"
            android:layout_marginBottom="5dp"
            app:layout_constraintBottom_toTopOf="@+id/etalamat"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">
            <Button
                android:id="@+id/btnmaps"
                android:layout_width="160dp"
                android:layout_height="match_parent"
                android:background="@drawable/button1"
                android:fontFamily="@font/reguler"
                android:text="Select on maps   >>"
                android:drawableLeft="@drawable/ic_location_on_white_12dp"
                android:textColor="@color/white"
                android:textSize="12dp"
                android:padding="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="MissingConstraints" />
        </RelativeLayout>
        <EditText
            android:id="@+id/etalamat"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/input_field"
            android:fontFamily="@font/reguler"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:hint="Car Salon Address"
            android:inputType="text"
            app:layout_constraintBottom_toTopOf="@+id/tvprice"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent" />
        <TextView
            android:id="@+id/tvprice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/reguler"
            android:text="Price Car :"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/ethargaMobil"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.25"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/ethargaMobil"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/input_field"
            android:fontFamily="@font/reguler"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:hint="Price Car"
            android:inputType="number"
            app:layout_constraintBottom_toTopOf="@+id/tvhargaMotor"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/tvhargaMotor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/reguler"
            android:text="Price Bike :"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/ethargaMotor"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.245"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/ethargaMotor"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/input_field"
            android:fontFamily="@font/reguler"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:hint="Price Bike"
            android:inputType="number"
            app:layout_constraintBottom_toTopOf="@+id/tvJamBuka"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/tvJamBuka"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/reguler"
            android:text="Jam Buka : "
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/etjamBuka"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.255"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/etjamBuka"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginBottom="12dp"
            android:background="@drawable/input_field"
            android:fontFamily="@font/reguler"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:hint="Jam Buka"
            android:inputType="time"
            app:layout_constraintBottom_toTopOf="@+id/tvJamTutup"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/tvJamTutup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/reguler"
            android:text="Jam Tutup :"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/etjamTutup"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.251"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/etjamTutup"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:background="@drawable/input_field"
            android:fontFamily="@font/reguler"
            android:hint="Jam Tutup"
            android:inputType="time"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/description"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/description"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/reguler"
            android:text="Input Description :"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintBottom_toTopOf="@+id/etdescription"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.282"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/etdescription"
            android:layout_width="250dp"
            android:layout_height="60dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/input_field"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:fontFamily="@font/reguler"
            android:hint="Description Car Salon"
            android:inputType="textLongMessage"
            app:layout_constraintBottom_toTopOf="@+id/btnRegister"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent" />
        <Button
            android:id="@+id/btnRegister"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginBottom="40dp"
            android:background="@drawable/button"
            android:fontFamily="@font/reguler"
            android:text="Register"
            android:textColor="@color/white"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

        <ProgressBar
            android:id="@+id/progressBar"
            android:visibility="invisible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>